@model DbdataforStockVolatility
@using (Html.BeginForm("EditStock", "DbdataforStockVolatility", FormMethod.Post))
    {
@*<form asp-controller="DbdataforStockVolatility" asp-action="ChangeStock" method="post">*@
    <div class="form-group">
        <label>name</label>
        <input type="text" asp-for="name" class="form-control" id="name">
    </div>
    <div class="form-group">
        <label>sector</label>
        <input type="text" asp-for="sector" class="form-control" id="sector">
    </div>
    <div class="form-group">
        @*<label>stockIdTicker</label>*@
        <input type="text" asp-for="stockIdTicker" class="form-control" id="stockIdTicker" value="@Model.stockIdTicker" hidden>
    </div>
    <div class="form-group">
        <label>stockVolatility</label>
        <input type="text" asp-for="stockVolatility" class="form-control" id="stockVolatility">
    </div>

    <div class="form-group">
        <input hidden type="text" asp-for="surveyInfoTable" value="[]" class="form-control" id="surveyInfoTable">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    }

